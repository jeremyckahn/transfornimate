{
  "name": "Transfornimate",
  "description": "Animates elements on a page",
  "version": "0.0.1",
  "permissions": ["contextMenus"],
  "background": {
    "persistent": false,
    "scripts": ["src/init.js"]
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["src/content.js"]
  }],
  "manifest_version": 2
}
